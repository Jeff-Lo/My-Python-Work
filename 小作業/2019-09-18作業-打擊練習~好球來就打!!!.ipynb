    "import numpy as np\n",
    "print(\"圖片是投球的25宮格，綠色區域是好球，紅色區域是壞球。\")\n",
    "msg=\"\"\n",
    "while(msg!=\"開打!!!\"):\n",
    "    P=np.random.randint(1,26)\n",
    "    print(\"投手投出的球:\")\n",
    "    print(P)\n",
    "    if (1<=P<=9):\n",
    "        msg=print(\"開打!!!\")\n",
    "        break\n",
    "    elif (10<=P<=25):\n",
    "        msg=print(\"我不打~\")"
